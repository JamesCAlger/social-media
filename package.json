{
  "name": "social-media-pipeline",
  "version": "1.0.0",
  "description": "Automated social media content generation pipeline for ASMR videos",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch src --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "pipeline": "ts-node src/index.ts",
    "setup-db": "ts-node scripts/setup-db.ts",
    "migrate": "ts-node scripts/migrations/001-multi-account.ts",
    "review": "ts-node scripts/review-content.ts",
    "test-layer4": "ts-node scripts/test-layer4.ts",
    "setup-telegram": "ts-node scripts/setup-telegram.ts",
    "test-telegram": "ts-node scripts/test-telegram.ts",
    "check-poller": "ts-node scripts/check-poller.ts",
    "clean-poller-lock": "ts-node scripts/clean-poller-lock.ts",
    "telegram-poller": "npm run check-poller && ts-node scripts/telegram-poller-service.ts",
    "telegram-poller:force": "ts-node scripts/telegram-poller-service.ts",
    "test-google-drive": "ts-node scripts/test-google-drive.ts",
    "test-instagram": "ts-node scripts/test-instagram.ts",
    "cron": "ts-node src/cron.ts",
    "cron:prod": "node dist/cron.js",
    "accounts": "ts-node src/cli/manage-accounts.ts",
    "accounts:list": "ts-node src/cli/manage-accounts.ts list",
    "accounts:create": "ts-node src/cli/manage-accounts.ts create",
    "accounts:setup-test": "ts-node src/cli/manage-accounts.ts setup-test",
    "run:account": "ts-node src/cli/run-account.ts --account",
    "run:all": "ts-node src/cli/run-account.ts --all",
    "run:due": "ts-node src/cli/run-account.ts --due",
    "run:status": "ts-node src/cli/run-account.ts --status",
    "scheduler": "ts-node src/services/scheduler.ts",
    "analytics": "ts-node src/cli/analytics.ts",
    "analytics:report": "ts-node src/cli/analytics.ts report",
    "analytics:collect": "ts-node src/cli/analytics.ts collect",
    "analytics:compare": "ts-node src/cli/analytics.ts compare",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "social-media",
    "automation",
    "asmr",
    "video-generation",
    "content-pipeline"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@aws-sdk/client-s3": "^3.937.0",
    "@fal-ai/serverless-client": "^0.14.3",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "fluent-ffmpeg": "^2.1.3",
    "googleapis": "^164.1.0",
    "node-cron": "^3.0.3",
    "openai": "^4.68.4",
    "pg": "^8.13.1",
    "uuid": "^11.0.3",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
